<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tealium.sample" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="TealiumPg">
        <param name="android-package" value="org.apache.cordova.plugin.TealiumPg" />
    </feature>
    <allow-intent href="market:*" />
    <name>TealiumSample</name>
    <description>
        A sample Apache Cordova application with the Tealium Plugin added. Includes two buttons to manually trigger event and a view calls.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <feature name="TealiumCompact">
        <param name="android-package" value="org.apache.cordova.plugin.TealiumPg" />
    </feature>
</widget>
